{
  "name": "react-boilerplate",
  "version": "0.12.0",
  "description": "A boilerplate for use compiling static applications consisting of ES6 source using Babel and Webpack. Comes pre-configured to use React, React Router, Reflux, Bootstrap, and LESS.",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rimraf -rf build",
    "build:development": "webpack --config ./config/webpack.development.js",
    "build:production": "webpack --config ./config/webpack.production.js",
    "build": "npm run build:development",
    "start": "webpack-dev-server -c -d -h --progress --config ./config/webpack.hot.js"
  },
  "keywords": [
    "webpack",
    "react",
    "reactjs",
    "reflux",
    "react-router",
    "babel",
    "bootstrap",
    "es6"
  ],
  "author": {
    "name": "Michael Diolosa",
    "email": "michael.diolosa@gmail.com",
    "url": "https://twitter.com/mbrio"
  },
  "contributors": [],
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git@github.com:mbrio/react-boilerplate.git"
  },
  "homepage": "https://github.com/mbrio/react-boilerplate",
  "bugs": "https://github.com/mbrio/react-boilerplate/issues",
  "devDependencies": {
    "babel-core": "^4.7.1",
    "babel-loader": "^4.1.0",
    "bootstrap": "^3.3.2",
    "classnames": "^1.1.4",
    "css-loader": "^0.9.1",
    "file-loader": "^0.8.1",
    "immutable": "^3.6.4",
    "jquery": "^2.1.3",
    "jshint": "^2.6.3",
    "jsxhint": "^0.12.1",
    "jsxhint-loader": "^0.2.0",
    "less": "^2.4.0",
    "less-loader": "^2.1.0",
    "react": "^0.12.2",
    "react-hot-loader": "^1.1.7",
    "react-router": "^0.12.4",
    "react-tools": "^0.12.2",
    "reflux": "^0.2.7",
    "rimraf": "^2.3.2",
    "script-loader": "^0.6.1",
    "style-loader": "^0.8.3",
    "url-loader": "^0.5.5",
    "webpack": "^1.7.2",
    "webpack-dev-server": "^1.7.0",
    "whatwg-fetch": "^0.7.0"
  }
}
